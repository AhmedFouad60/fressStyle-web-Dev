doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    ul
      li My normal text
      input(type="text").anyclass
    if(title)
      p title exists
    - var condition = true;
    if(condition)
      p true

    - var anyArray = [1,2,3]
    each value in anyArray
      p= value
    block content
